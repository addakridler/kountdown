{
  "manifest_version": 3,
  "name": "komodoro",
  "version": "0.0.1",
  "description": "The komodoro timer that will drive you bananas",
  "author": "Adda Kridler, Hasan Ozutemiz",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "alarms"],
  "action": {
    "default_popup" : "popup.html",
    "default_icon" : {
      "128" : "komodoro.png"
    }
  },
  "icons" : {
    "128": "komodoro.png"
  }
}